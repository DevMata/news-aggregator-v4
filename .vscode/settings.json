{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.js": true,
    "**/*.js.map": true,
    "**/*.d.ts": true
  },
  "editor.codeActionsOnSave": { "source.fixAll": true },
  "eslint.validate": ["typescript"],
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.formatOnSave": false
  }
}
